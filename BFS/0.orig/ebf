FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ebf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    "(inlet|outlet)"
    {
      type      zeroGradient;
    }

    "(lowerWallStartup|upperWallStartup)"
    {
        type            symmetryPlane;
    }

    "(upperWall|lowerWall)"
    {
        type      fixedValue;
        value     uniform 0;
    }

    "(front|back)"
    {
        type            empty;
    }
}


